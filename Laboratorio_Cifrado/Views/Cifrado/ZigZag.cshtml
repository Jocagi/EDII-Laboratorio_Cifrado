
@{
    ViewBag.Title = "ZigZag";
}

<h2>ZigZag</h2>
<div class="jumbotron">
    <h3>Subir archivo a Cifrar</h3>
</div>

@using (Html.BeginForm("CifrarZigZag", "Cifrado", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <label for="file">Subir archivo:</label>
    <input type="file" name="file" id="file" /><br><br>
    <input type="submit" value="Upload File" />
    <input type="text" name="Corriemiento" id="Corrimiento"/> <br />
    //<input type="submit" value=""

    <br><br>
    @ViewBag.Message
}

